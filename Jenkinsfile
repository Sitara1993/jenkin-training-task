pipeline {
  agent any
  stages {
    stage ("Checkout"){
      steps{
        echo 'Pulling the code from the dev branch'
      }
    }
    stage ("Build"){
      steps{
        echo 'Building the application'
      }
    }
    stage ("Test"){
      steps{
        echo 'Testing the application'
      }   
    }
stage ("Deploy"){
      steps{
        echo 'Deploing the application'
      } 
}
}
}
